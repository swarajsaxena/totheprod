[
  {
    "componentName": "TtpSubscriptionCalendar",
    "props": {
      "subscriptions": {
        "type": "TSubscription[]",
        "description": "Array of subscription objects to display on the calendar",
        "required": true
      },
      "currency": {
        "type": "string",
        "description": "Currency symbol or code to display",
        "default": "Rs",
        "required": false
      },
      "className": {
        "type": "string",
        "description": "Custom class name for the calendar container",
        "required": false
      },
      "cellClassName": {
        "type": "string",
        "description": "Custom class name for individual day cells",
        "required": false
      },
      "modalClassName": {
        "type": "string",
        "description": "Custom class name for the modal",
        "required": false
      },
      "showMonthTotal": {
        "type": "boolean",
        "description": "Show/hide the month total",
        "default": "true",
        "required": false
      },
      "showIndicators": {
        "type": "boolean",
        "description": "Show/hide the subscription type indicators",
        "default": "true",
        "required": false
      },
      "colors": {
        "type": "{ monthly?: string; yearly?: string; both?: string }",
        "description": "Custom colors for subscription types",
        "required": false
      },
      "animationConfig": {
        "type": "{ stiffness?: number; damping?: number }",
        "description": "Animation configuration",
        "required": false
      },
      "renderSubscriptionItem": {
        "type": "(subscription: TSubscription) => React.ReactNode",
        "description": "Custom render function for subscription items in modal",
        "required": false
      }
    }
  }
]
